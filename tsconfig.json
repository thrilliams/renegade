{
	"compilerOptions": {
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"module": "esnext",
		"target": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"downlevelIteration": true,
		"skipLibCheck": true,
		"jsx": "preserve",
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true
	}
}
